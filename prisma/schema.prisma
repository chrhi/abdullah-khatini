

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}





model Product {
  id                                 String   @id @default(uuid())
  createdAt                          DateTime @default(now())
  name                               String
  description                        String?
  status                             String   @default("PUBLSIHED")
  images                             Json?
  price                              String

}
